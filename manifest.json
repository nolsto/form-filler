{
  "manifest_version": 2,
  "name": "Form Filler",
  "description": "Fills forms",
  "version": "1.0.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ],
  "optional_permissions": [
    "http://*/",
    "https://*/"
  ]
}
